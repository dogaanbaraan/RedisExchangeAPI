@model HashSet<string>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<form method="post" asp-action="Add" enctype="application/x-www-form-urlencoded">
    <input type="text" name="name" />
    <input type="number" name="score" />
    <button type="submit"> Kayıt Et</button>
</form>

@foreach (var item in Model)
{
    <h2>@item <a asp-controller="SortedSetType" asp-action="DeleteItem" asp-route-name="@item">sil</a>  </h2>
}
